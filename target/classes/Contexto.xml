<?xml version="1.0" encoding="UTF-8"?>
<beans 
	xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
    "
    >
    
	<bean id="gocClub" class="ModeloImp.Club"></bean>
	<bean id="socio" class="ModeloImp.Socio" scope="prototype"></bean>
	<bean id="autorizado" class="ModeloImp.Autorizado" scope="prototype"></bean>
	<bean id="factura" class="ModeloImp.Factura"  scope="prototype"></bean>
	
	<bean id= "controladorBussiness" class="ControladoresImp.ControladorBussinessImp"></bean>
	<bean id= "controladorBussinessInput" class="ControladoresImp.ControladorBussinessInputImp"></bean>
	<bean id= "controladorInput" class="ControladoresImp.ControladorInputImp"></bean>

</beans>